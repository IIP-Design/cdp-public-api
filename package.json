{
  "name": "cdp-public-api",
  "version": "1.4.1",
  "main": "server.js",
  "repository": "git@github.com:IIP-Design/cdp-public-api.git",
  "author": "Office of Design <designdevops@america.gov>",
  "license": "MIT",
  "scripts": {
    "build:clear": "rimraf build",
    "build:worker": "webpack --config webpack.worker.js --mode production --progress --colors",
    "build:main": "webpack --mode production --progress --colors",
    "build": "npm run build:clear && npm run build:worker && npm run build:main",
    "start": "npm run start:main & npm run start:worker",
    "start:worker": "node build/consumer.js",
    "start:main": "node build/server.js",
    "dev": "nodemon -e js -x babel-node --inspect=7000 src/index.js",
    "dev:worker": "nodemon -e js -x babel-node --inspect=7001 src/workers/consumer.js"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "amqplib": "^0.5.3",
    "archiver": "^2.1.1",
    "aws-sdk": "^2.491.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bodybuilder": "^2.2.17",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^4.4.6",
    "cuid": "^2.1.6",
    "dotenv": "^8.0.0",
    "elastic-apm-node": "^1.14.0",
    "elasticsearch": "^15.1.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "get-stream": "^5.1.0",
    "helmet": "^3.21.2",
    "http-aws-es": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "mammoth": "^1.4.9",
    "mime-types": "^2.1.24",
    "newrelic": "^6.2.0",
    "node-html-to-image": "^1.1.0",
    "request": "^2.88.0",
    "source-map-support": "^0.5.12",
    "tmp": "0.0.33",
    "tus-js-client": "^1.5.1",
    "vimeo": "^2.1.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.18.0",
