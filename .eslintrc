  "parser": "@babel/eslint-parser",
  "settings": {
    "import/resolver": {
      "babel-module": {
        "alias": {
          "api": "./src/api",
          "middleware": "./src/middleware",
          "services": "./src/services",
          "workers": "./src/workers"
        }
      }
    }
  }
}
